%nav
  .container
    .row
      - if logged_in?
        .col-6-sm.left
          %a{:href => "#"}= current_user.first_name
        .col-6-sm.right
          = link_to "Log Out", logout_path
      - else
        .col-6-sm.left
          = link_to "Sign Up", new_user_path
        .col-6-sm.right
          = link_to "Log In", login_path
