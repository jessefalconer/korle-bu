.col-12.center
  %h3 Create an account
.col-12.center
  = form_for @user do |f|
    = f.label :email
    %br
    = f.text_field :email
    %br
    = f.label :first_name
    %br
    = f.text_field :first_name
    %br
    = f.label :last_name
    %br
    = f.text_field :last_name
    %br
    = f.label :password
    %br
    = f.password_field :password
    %br
    %br
    = f.submit
