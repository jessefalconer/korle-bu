%h5 Search for an existing item in the database to stage.
.input-group.search.span12
  = hidden_field_tag :model, "Warehouse", id: "search-model"
  = hidden_field_tag :status, "Warehouse", id: "status"
  .input-group
    = text_field_tag :search, params[:search], placeholder: "Begin typing to search...", class: "form-control", id: "search-params"
    %span.input-group-btn.span12#packing-search-submit
      %a.btn.btn-primary.responsive-button
        %span Search
        %i.fa.fa-search

#search-results
= render partial: "layouts/estimate_modal", locals: {title: "Weight or Quantity Estimate", body: ""}
